{
  "name": "japont-loader",
  "version": "0.0.1",
  "author": "3846masa",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-core": "6.11.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-builtin-extend": "1.1.0",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-runtime": "6.9.2",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "eslint": "3.1.1",
    "eslint-config-standard": "5.3.5",
    "eslint-plugin-promise": "2.0.0",
    "eslint-plugin-standard": "2.0.0",
    "fixpack": "2.3.1",
    "licensify": "3.1.2",
    "minifyify": "7.3.3",
    "npm-run-all": "2.3.0",
    "uglifyify": "3.0.2",
    "watchify": "3.7.0"
  },
  "license": "MIT",
  "main": "japont.js",
  "scripts": {
    "build": "npm-run-all build:browserify",
    "build:browserify": "browserify -vd -p licensify -p [ minifyify --no-map --uglify [ --mangle --compress ] ] -e japont.js > ../templates/japont.min.js",
    "fixpack": "fixpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-run-all watch:browserify",
    "watch:browserify": "watchify -vd -p licensify -p [ minifyify --map japont.min.js.map --output japont.min.js.map --uglify [ --mangle --compress ] ] -e japont.js -o ../templates/japont.min.js"
  },
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  }
}
